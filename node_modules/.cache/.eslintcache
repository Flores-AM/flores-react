.js",["76"],[],"import React from \"react\";\nimport \"../../../_generales.scss\";\nimport \"./item.scss\";\nimport Counter from \"./counter/Counter\";\n\nconst Item = ({ item }) => {\n  const { id, name, price, image } = item;\n\n  return (\n    <div className=\"item\">\n      <img src={image} alt={name} />\n      <h5> {name} </h5>\n      <p> {price} </p>\n      <Counter />\n    </div>\n  );\n};\n\nexport default Item;\n","/Users/usuario/Desktop/react-Flores/my-react-app/src/components/views/inicio/Inicio.js",[],[],"/Users/usuario/Desktop/react-Flores/my-react-app/src/components/views/usuarios/Usuarios.js",["77"],[],"import React, { useEffect, useState } from \"react\";\nimport \"../../_generales.scss\";\nimport \"./usuarios.scss\";\n\nconst Usuarios = () => {\n  const [user, setUser] = useState(null);\n  const [loading, setLoading] = useState(true);\n\n  useEffect(() => {\n    fetch(\"https://api.randomuser.me/\")\n      .then((res) => res.json())\n      .then((data) => {\n        console.log(data);\n\n        const userData = {\n          name: data.results[0].name.first,\n          email: data.results[0].email,\n          picture: data.results[0].picture.thumbnail,\n        };\n\n        setUser(userData);\n        setLoading(false);\n      });\n  }, []);\n\n  return (\n    <div className=\"todo\">\n      {loading ? (\n        <div class=\"spinner\">\n          <span></span>\n          <span></span>\n          <span></span>\n        </div>\n      ) : null}\n      {user ? (\n        <div className=\"caja-datos\">\n          <div className=\"caja-img\">\n            <img src={user.picture} />\n          </div>\n          <div className=\"datos\">\n            <h4>{user.name}</h4>\n            <p>{user.email}</p>\n          </div>\n        </div>\n      ) : null}\n    </div>\n  );\n};\n\nexport default Usuarios;\n","/Users/usuario/Desktop/react-Flores/my-react-app/src/components/views/container/item/counter/Counter.js",["78"],[],"import React, { useState } from \"react\";\n// import \"./counter.css\";\nimport \"../../../../_generales.scss\";\nimport \"./counter.scss\";\n\nconst Counter = () => {\n  const [numero, setNumero] = useState(0);\n  const [bool, setBool] = useState(false);\n  const [btn, setBtn] = useState(\"Add To Cart\");\n\n  const suma = () => {\n    if (numero === 10) {\n      setBool(true);\n    } else {\n      setNumero(numero + 1);\n    }\n  };\n\n  const resta = () => {\n    if (numero === 0) {\n      setBool(true);\n    } else {\n      setNumero(numero - 1);\n    }\n  };\n\n  const handleBtn = () => {\n    if (numero >= 1) {\n      setBtn(\"Added\");\n    }\n\n    setTimeout(() => {\n      setBtn(\"Add To Cart\");\n    }, 3000);\n  };\n\n  return (\n    <section className=\"carta\">\n      <div className=\"botonera\">\n        <button onClick={resta}>-</button>\n        <p>{numero}</p>\n        <button onClick={suma}>+</button>\n      </div>\n      <button className=\"btn-add\" onClick={handleBtn}>\n        {btn}\n      </button>\n    </section>\n  );\n};\n\nexport default Counter;\n",{"ruleId":"79","severity":1,"message":"80","line":2,"column":8,"nodeType":"81","messageId":"82","endLine":2,"endColumn":14},{"ruleId":"79","severity":1,"message":"83","line":5,"column":10,"nodeType":"81","messageId":"82","endLine":5,"endColumn":14},{"ruleId":"79","severity":1,"message":"84","line":2,"column":8,"nodeType":"81","messageId":"82","endLine":2,"endColumn":15},{"ruleId":"79","severity":1,"message":"85","line":1,"column":10,"nodeType":"81","messageId":"82","endLine":1,"endColumn":21},{"ruleId":"79","severity":1,"message":"86","line":1,"column":17,"nodeType":"81","messageId":"82","endLine":1,"endColumn":26},{"ruleId":"79","severity":1,"message":"87","line":1,"column":28,"nodeType":"81","messageId":"82","endLine":1,"endColumn":36},{"ruleId":"79","severity":1,"message":"88","line":7,"column":11,"nodeType":"81","messageId":"82","endLine":7,"endColumn":13},{"ruleId":"89","severity":1,"message":"90","line":38,"column":13,"n